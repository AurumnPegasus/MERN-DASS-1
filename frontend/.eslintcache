Apply] = useState(null)\n\n    const jobBanao = () => {\n        if(create) {\n            return <Redirect to='/createjob'></Redirect>\n        }\n    }\n\n    const editJob = () => {\n        if(edit) {\n            return <Redirect to='/editjob'></Redirect>\n        }\n    }\n\n    const deleteJob = () => {\n        if(del) {\n            return <Redirect to='/deletejob'></Redirect>\n        }\n    }\n\n    const viewJob = (title) => {\n        setView(true)\n        getData(title)\n    }\n\n    useEffect(() => {\n        async function timepass() {\n            if(loader) return\n            if (store.user !== undefined) {\n                const jobList = await axios.post('http://localhost:3000/jobs/ralljob', {\n                    email: store.user.email\n                })\n                setJobs(jobList.data.jobs)\n                console.log(jobs)\n                setLoader(true)\n            }\n        }\n\n        timepass()\n    })\n\n    const displaySkill = (skill, sidx) => {\n        try {\n            if (skill.name.length === 0) return\n            return (\n                <div>\n                    <p>\n                        <b>{`Skill ${sidx + 1}: `}</b> {skill.name}\n                    </p>\n                </div>\n            )\n        } catch (err) {\n            console.log(err)\n        }\n    }\n\n    const displayJobs = (job, idx) => {\n        try {\n            return (\n                <div>\n                    <p>\n                        <b>{`Job ${idx + 1} title: `}</b> {job.title}\n                    </p>\n                    <p>\n                        <b>{`by: `}</b> {job.name}\n                    </p>\n                    <p>\n                        <b>{`mail: `}</b> {job.email}\n                    </p>\n                    <p>\n                        <b> Deadline: </b> {job.deadline}\n                    </p>\n                    <p>\n                        <b>{`max applicants: `}</b> {job.applicants}\n              